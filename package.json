{
  "name": "messagenger-react",
  "version": "0.0.2",
  "description": "Messenger app with React",
  "main": "index.js",
  "engines": {
    "node": "^16"
  },
  "scripts": {
    "start": "npm run dev:app",

    "dev": "webpack-dev-server --host 0.0.0.0 --config webpack.dev.app.js",
    "prod": "NODE_ENV=production webpack --config webpack.prod.app.js --progress --profile --colors",

    "clean": "rimraf dist",
    "production": "cross-env npm run clean && webpack --config webpack.production.config.js --progress --profile --colors",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix --ext .js ./src",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "precommit": "cross-env npm run lint && npm test",
    "prepush": "cross-env npm run lint && npm test"
  },
  "author": "Ivan Proskuryakov <volgodark@gmail.com>",
  "license": "MIT",
  "jest": {
    "verbose": true,
    "setupFiles": [
      "<rootDir>/config/jest/shim.js"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/jest/assetsTransformer.js",
      "\\.(css|less)$": "<rootDir>/config/jest/assetsTransformer.js",
      "^api(.*)$": "<rootDir>/src/api$1",
      "^action(.*)$": "<rootDir>/src/redux/action$1",
      "^assets(.*)$": "<rootDir>/src/assets$1",
      "^styles(.*)$": "<rootDir>/src/styles$1",
      "^components(.*)$": "<rootDir>/src/components$1",
      "^containers(.*)$": "<rootDir>/src/containers$1",
      "^config(.*)$": "<rootDir>/src/config$1",
      "^helper(.*)$": "<rootDir>/src/helper$1",
      "^reducer(.*)$": "<rootDir>/src/redux/reducer$1",
      "^service(.*)$": "<rootDir>/src/service$1",
      "^store(.*)$": "<rootDir>/src/redux/store$1",
      "^type(.*)$": "<rootDir>/src/redux/type$1"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "ts",
      "tsx",
      "node",
      "bundle-loader?lazy!."
    ],
    "testMatch": [
      "**/?(*.)(spec|test).js?(x)"
    ],
    "transform": {
      "\\.js$": "babel-jest"
    }
  },
  "dependencies": {
    "@babel/cli": "7.17.6",
    "@babel/core": "^7.17.5",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-decorators": "^7.17.2",
    "@babel/plugin-proposal-export-default-from": "^7.16.7",
    "@babel/plugin-proposal-export-namespace-from": "^7.16.7",
    "@babel/plugin-proposal-function-bind": "^7.16.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.17.3",
    "@babel/plugin-proposal-optional-chaining": "^7.16.7",
    "@babel/plugin-transform-flow-strip-types": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-react": "7.16.7",
    "@babel/preset-stage-2": "7.8.3",
    "@babel/register": "7.17.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.6",

    "@material-ui/pickers": "^3.3.10",
    "@mui/icons-material": "^5.5.0",
    "@mui/lab": "^5.0.0-alpha.72",
    "@mui/material": "^5.5.0",
    "@mui/styles": "^5.5.0",
    "lodash": "^4.17.21",

    "axios": "^0.26.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "^27.5.1",
    "babel-loader": "8.2.3",
    "babel-preset-react-hmre": "1.1.1",
    "babel-plugin-lodash": "^3.3.4",
    "cross-env": "7.0.3",
    "css-hot-loader": "1.4.4",
    "css-loader": "6.7.0",
    "emoji-mart": "^3.0.1",
    "enzyme": "3.11.0",
    "eslint": "^8.10.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.3",
    "eslint-webpack-plugin": "^3.1.1",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.5.0",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "js-cookie": "^3.0.1",
    "moment": "^2.29.1",
    "node-sass": "7.0.1",
    "open-browser-webpack-plugin": "0.0.5",
    "prop-types": "^15.8.1",
    "pusher-js": "^7.0.6",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "@hot-loader/react-dom": "^17.0.2",
    "react-hot-loader": "4.13.0",
    "react-moment": "^1.1.1",
    "react-redux": "^7.2.6",
    "react-router-dom": "6.2.2",
    "react-scroll": "^1.8.6",
    "react-spinners": "^0.11.0",
    "react-test-renderer": "17.0.2",
    "react-textarea-autosize": "^8.3.3",
    "redux": "^4.1.2",
    "regenerator-runtime": "0.13.9",
    "rimraf": "3.0.2",
    "sass-loader": "12.6.0",
    "style-loader": "3.3.1",
    "typeface-roboto": "1.1.13",
    "url-loader": "4.1.1",

    "webpack": "5.70.0",
    "webpack-cli": "4.9.2",
    "webpack-dev-server": "4.7.4"
  }
}
